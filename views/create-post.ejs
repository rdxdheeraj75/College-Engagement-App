<%- include("partials/header.ejs") -%>
<%- include("partials/navbar.ejs") -%>
<h1>Compose</h1>

<form action="/create-post" method="post">
  <div class="form-group ">
    <label>Title</label>
    <input class="form-control" type="text" name="postTitle">

    <label>Type</label>
    <input class="form-control" type="text" name="postType">

    <label>Post</label>
    <textarea class="form-control" name="postBody" rows="4" cols="30"></textarea>
    
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>


<%- include("partials/footer.ejs") -%>